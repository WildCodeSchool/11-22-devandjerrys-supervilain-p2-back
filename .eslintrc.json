{
	"plugins": ["prettier"],
	"extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
	"env": { "browser": true, "node": true, "jest": true },
	"rules": {
		"prettier/prettier": "warn",
		"no-unused-vars": 1
	},
	"parserOptions": {
    "sourceType": "module",
		"ecmaVersion": 2015 
  }
}
